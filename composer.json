{
    "name": "cmath10/mapper",
    "description": "Mapping library",
    "keywords": ["mapper", "automapper"],
    "type": "library",
    "version": "1.0.2",
    "minimum-stability": "stable",
    "license": "MIT",
    "homepage": "https://github.com/cmath10/mapper",
    "require": {
        "php": ">=7.4",
        "symfony/property-access": "^4.4 || ^5.0",
        "symfony/expression-language": "^4.4 || ^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "cmath10\\Mapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cmath10\\Mapper\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check"
        ],
        "cs-check": "php-cs-fixer fix -v --using-cache=no --dry-run",
        "cs-fix": "php-cs-fixer fix -v --using-cache=no"
    },
    "config": {
        "sort-packages": true
    }
}
